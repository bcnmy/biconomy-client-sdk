{
  "name": "@biconomy/particle-auth",
  "version": "3.1.2",
  "description": "Particle auth for Biconomy SDK",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "legos",
    "batching",
    "one-click",
    "cross-chain",
    "particle",
    "particle-auth"
  ],
  "author": "Biconomy",
  "homepage": "https://github.com/bcnmy/biconomy-client-sdk#readme",
  "license": "MIT",
  "files": [
    "dist/*",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcnmy/biconomy-client-sdk.git"
  },
  "scripts": {
    "unbuild": "rimraf dist *.tsbuildinfo",
    "build:watch": "node .esbuild.js CJS --watch",
    "build": "yarn unbuild && yarn build:cjs && yarn build:esm && yarn build:typ",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --outDir ./dist/cjs --removeComments --verbatimModuleSyntax false && echo > ./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
    "build:esm": "tsc --project tsconfig.build.esm.json --module esnext --outDir ./dist/esm --removeComments && echo > ./dist/esm/package.json '{\"type\":\"module\"}'",
    "build:typ": "tsc --project tsconfig.build.json --module esnext --declarationDir ./dist/types --emitDeclarationOnly --declaration --declarationMap",
    "build:tsc": "rimraf dist && tsc",
    "format": "prettier --write \"{src,tests}/**/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "bugs": {
    "url": "https://github.com/bcnmy/biconomy-client-sdk/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@particle-network/auth": "^1.2.1",
    "@particle-network/biconomy": "^1.0.0",
    "@particle-network/provider": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.10",
    "esbuild": "^0.19.11",
    "esbuild-plugin-tsc": "^0.4.0",
    "npm-dts": "^1.3.12"
  }
}
